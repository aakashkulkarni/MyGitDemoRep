<p id="notice"><%= notice %></p>

<h1>Binaries</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @binaries.each do |binary| %>
      <tr>
        <td><%= binary.title %></td>
        <td><%= binary.description %></td>
        <td><%= link_to 'Show', binary %></td>
        <td><%= link_to 'Edit', edit_binary_path(binary) %></td>
        <td><%= link_to 'Destroy', binary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Binary', new_binary_path %>
